drupal_add_li­brary(‚system‘, ‚ui.datepicker‘); +&lt;/p&gt;

&lt;ul&gt;
	&lt;li&gt;// Add i18n support&lt;/li&gt;

	&lt;li&gt;if ($GLOBALS[‚lan­guage‘]-&amp;gt;language != ‚en‘) {&lt;/li&gt;

	&lt;li&gt;// D7.x: &lt;a href=&quot;https://jquery-ui.googlecode.com/svn/tags/1.8.7/ui/i18n/&quot;&gt;https://jquery-ui.googlecode.com/…8.7/ui/i18n/&lt;/a&gt;&lt;/li&gt;

	&lt;li&gt;drupal_add_js( drupal_get_pat­h(‚module‘, ‚date_popup‘) .
	‚/jquery.ui.da­tepicker-‘ . $GLOBALS[‚lan­guage‘]-&amp;gt;language .&lt;/li&gt;
